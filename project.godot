config_version=5

[application]

config/name="FantasyBulletHell"
run/main_scene="res://Assets/Scenes/Main/main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60
config/icon="res://Assets/Misc/icon.svg"

[autoload]

BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
Globals="*res://Assets/Singletons/Autoload/Globals.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/Scenes/Bullets and Spawners/": "yellow",
"res://Assets/Scenes/Main/": "red",
"res://Assets/Scenes/Stages/": "purple",
"res://Assets/Scenes/Temp Player/": "blue",
